{
    "name": "simple-swoole/rpc-multiplex",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "",
    "autoload": {
        "psr-4": {
            "Simps\\RpcMultiplex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.3",
        "multiplex/socket": "^0.1",
        "simple-swoole/simps": "^1.0",
        "simple-swoole/consul": "^0.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": ">=7.0",
        "swoole/ide-helper": "dev-master",
        "symfony/var-dumper": "^5.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 -c phpstan.neon ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    }
}
